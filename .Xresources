Xft.dpi:        96
Xft.antialias:  1
Xft.hinting:    1
Xft.hintstyle:  hintmedium
URxvt*depth: 32
URxvt.font:            xft:FiraCode Nerd Font Mono:size=18, xft:FontAwesome:size=6
URxvt.scrollBar_right: false
URxvt.scrollBar:       false
URxvt.transparent:     false
URxvt.letterSpace:     0
URxvt.lineSpace:       0
URxvt.geometry: 200x200

URxvt.iso14755:    false
URxvt.iso14755_52: false

URxvt*foreground: #e4e4ef
URxvt*background: #181818
URxvt*color0:     #2E3436
URxvt*color1:     #a40000
URxvt*color2:     #4E9A06
URxvt*color3:     #C4A000
URxvt*color4:     #3465A4
URxvt*color5:     #75507B
URxvt*color6:     #ce5c00
URxvt*color7:     #babdb9
URxvt*color8:     #555753
URxvt*color9:     #EF2929
URxvt*color10:    #8AE234
URxvt*color11:    #FCE94F
URxvt*color12:    #729FCF
URxvt*color13:    #AD7FA8
URxvt*color14:    #fcaf3e
URxvt*color15:    #EEEEEC

URxvt.keysym.C-minus:  resize-font:smaller
URxvt.keysym.C-equal:  resize-font:bigger
URxvt.keysym.C-0:      resize-font:reset

URxvt.perl-ext-common: default, eval

URxvt.keysym.C-0x43: eval:selection_to_clipboard
URxvt.keysym.C-0x56: eval:paste_clipboard
